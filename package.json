    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "sinon": "^7.3.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "/lib",
      "/**/fixtures"
    ]
