{
  "name": "babel-plugin-postcss-css-module",
  "version": "1.0.0",
  "scripts": {
    "build": "babel src -d lib --source-maps",
    "lint": "standard --fix",
    "test": "mocha"
  },
  "description": "For css module by babel & postcss",
  "main": "index.js",
  "repository": "git@github.com:teabyii/babel-plugin-postcss-css-module",
  "author": "teabyii <teabyii@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "postcss": "^7.0.14",
